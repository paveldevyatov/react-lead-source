{
  "name": "react-lead-source",
  "version": "1.0.6",
  "description": "Drop-in React component that captures UTM parameters, ad click IDs, referrer and device context into localStorage â€” so you always know which channel brought each lead.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "utm",
    "utm-tracking",
    "utm-parameters",
    "attribution",
    "first-touch",
    "last-touch",
    "lead-source",
    "marketing",
    "marketing-attribution",
    "referrer",
    "gclid",
    "fbclid",
    "msclkid",
    "ttclid",
    "ad-tracking",
    "visitor",
    "source",
    "analytics",
    "tracking",
    "localStorage",
    "nextjs",
    "next",
    "remix"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paveldevyatov/react-lead-source.git"
  },
  "homepage": "https://github.com/paveldevyatov/react-lead-source#readme",
  "bugs": {
    "url": "https://github.com/paveldevyatov/react-lead-source/issues"
  },
  "author": "Pavel Devyatov <p.devyatov@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
